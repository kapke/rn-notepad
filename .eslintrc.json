{
  "env": {
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "prettier",
    "fp",
    "react",
    "jest"
  ],
  "extends": ["eslint:recommended", "plugin:fp/recommended", "plugin:react/recommended", "plugin:jest/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}